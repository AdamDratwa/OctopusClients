{
  "name": "OctopusTools",
  "version": "0.0.0-*",
  "dependencies": {
    "Microsoft.NETCore.Runtime.CoreCLR": "1.0.2",
    "Microsoft.NETCore.DotNetHostPolicy": "1.0.1",  
    "System.ComponentModel.TypeConverter": "4.1.0",
    "System.Net.Http.WinHttpHandler": "4.0.0",
    "System.Xml.ReaderWriter": "4.0.11",
    "Autofac": "4.0.0",
    "ILRepack": "2.0.8",
    "Newtonsoft.Json": "9.0.1",
    "NuGet.CommandLine": "3.4.3",
    "NuGet.Common": "3.4.3",
    "NuGet.Frameworks": "3.4.3",
    "NuGet.Logging": "3.4.3",
    "NuGet.Packaging": "3.4.3",
    "NuGet.Packaging.Core": "3.4.3",
    "NuGet.Packaging.Core.Types": "3.4.3",
    "NuGet.Versioning": "3.4.3",
    "Octopus.Client": "3.4.0-dnc2411",
    "Octostache": "2.0.0-dnc0027",
    "Serilog": "2.1.0",
    "Serilog.Sinks.ColoredConsole": "2.0.0",
    "Serilog.Sinks.Trace": "2.0.0",
    "Sprache": "2.1.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "outputName": "Octo"
    // "warningsAsErrors": true
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },
  "runtimes": {
    "win7-x64": {},
    "ubuntu.14.04-x64": {},
    "ubuntu.16.04-x64": {}
  },
  "packOptions": {
    "tags": [ "automation", "deployment" ],
    "owners": [ "Octopus Deploy" ],
    "requireLicenseAcceptance": false,
    "summary": "Octopus Deploy is an automated deployment tool powered by NuGet. This package contains Octo.exe, a command line tool for creating and deploying releases with Octopus Deploy.",
    "projectUrl": "http://octopusdeploy.com/",
    "licenseUrl": "http://octopusdeploy.com/pricing",
    "iconUrl": "http://i.octopusdeploy.com/resources/Avatar3-32x32.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/OctopusDeploy/Octo.exe"
    }
  }
}